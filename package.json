{
  "name": "zeromq-ng",
  "version": "5.0.0-beta.10",
  "description": "Next-generation ZeroMQ bindings for Node.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "gypfile": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/rolftimmermans/zeromq-ng.git"
  },
  "binary": {
    "module_name": "zeromq-ng",
    "module_path": "./lib/binary/napi-v{napi_build_version}",
    "remote_path": "v{version}",
    "package_name": "{module_name}-{version}-{platform}-{arch}-{libc}-napi-v{napi_build_version}.tar.gz",
    "host": "https://github.com/rolftimmermans/zeromq-ng/releases/download/",
    "napi_versions": [
      1,
      2,
      3
    ]
  },
  "dependencies": {
    "node-addon-api": "^1.4",
    "node-pre-gyp": "^0.11"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "devDependencies": {
    "@types/node": ">= 8.0",
    "benchmark": ">= 0",
    "chai": ">= 4.1",
    "choma": ">= 1.2",
    "eslint": ">= 4.0",
    "mocha": ">= 4.0",
    "node-pre-gyp-github": ">= 1.3.1",
    "semver": ">= 0",
    "typescript": "^3.0",
    "weak": ">= 1.0"
  },
  "engines": {
    "node": ">= 8.8"
  },
  "scripts": {
    "install": "node-pre-gyp configure --build-latest-napi-version-only",
    "test": "node-pre-gyp build --build-latest-napi-version-only && eslint lib && script/format.sh && mocha && tsc --project test/unit",
    "bench": "node-pre-gyp build --build-latest-napi-version-only && node --use_strict --harmony_async_iteration --expose-gc test/bench"
  },
  "keywords": [
    "zeromq",
    "zmq",
    "0mq",
    "Ã¸mq",
    "libzmq",
    "native",
    "binding",
    "addon"
  ],
  "license": "MIT"
}
